{
    "name": "miika",
    "version": "1.0.0",
    "description": "Tell me about it.",
    "scripts": {
      "server-dev": "cd server && npm run dev",
      "client-dev": "cd client && npm run dev",
      "microservice-dev": "cd microservice && python3 app.py",
      "both-dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
      "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\" \"npm run microservice-dev\"",
      "server-install": "cd server && npm install",
      "client-install": "cd client && npm install",
      "microservice-install": "cd microservice && ./menv/bin/pip install --no-cache-dir -r requirements.txt",
      "install-dev": "npm install && npm run server-install && npm run client-install && npm run microservice-install"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/zhillan-arf/miika.git"
    },
    "author": "miiki_mikki",
    "license": "ISC",
    "bugs": {
      "url": "https://github.com/zhillan-arf/miika/issues"
    },
    "homepage": "https://github.com/zhillan-arf/miika#readme",
    "devDependencies": {
      "concurrently": "^8.2.2"
    }
  }
  